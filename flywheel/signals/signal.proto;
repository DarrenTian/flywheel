synctax = "proto2";

package signal;

message MetaSignal {
	optional string ticket_name = 1;
	optional BaseSignal base_signal = 2;
}

message BaseSignal {
	optional PriceSignal price_signal = 1;
}

message PriceSignal {
	repeated PriceEma price_ema = 1;
}

message PriceEma {
	optional int32 date = 1;
	optional double ema = 2;
}